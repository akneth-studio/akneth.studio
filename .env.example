BASE_KEY=your_base_key_here
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
NEXT_PUBLIC_PHONE="+48 123 456 789"
NEXT_PUBLIC_EMAIL=your@email.com
NEXT_PUBLIC_ADDRESS_STREET="ul. Przykładowa 1/1"
NEXT_PUBLIC_ADDRESS_CITY="00-000 Miasto"
NEXT_PUBLIC_CALENDAR="https://calendar.app.google/your-calendar-link"
